<div id="<%= dom_id member_activity %>">
  <p>
    <strong>User:</strong>
    <%= member_activity.user_id %>
  </p>

  <p>
    <strong>Activity:</strong>
    <%= member_activity.activity_id %>
  </p>

  <p>
    <strong>Date:</strong>
    <%= member_activity.date %>
  </p>

  <p>
    <strong>Start Time :</strong>
    <%= member_activity.start_time %>
  </p>

  <p>
    <strong>End Time :</strong>
    <%= member_activity.end_time %>
  </p>

</div>
